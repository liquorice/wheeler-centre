  "contextual/index.css": {
    "id": "contextual/index.css",
    "type": "css",
    "mtime": 1418884656000,
    "src": "/* Contextual */\n\n.contextual {\n  background-color: #fff;\n}\n\n  .contextual__section-title {\n    background-color: var(--primary-color);\n    color: var(--primary-color-lighter);\n    font-size: 17px;\n    font-size: 1.7rem;\n    font-weight: bold;\n    letter-spacing: 0.1em;\n    padding-left: 35px;\n    padding-top: 45px;\n    padding-bottom: 25px;\n    text-transform: uppercase;\n  }\n\n  .contextual__navigation-list {\n    float: left;\n    padding-left: 35px;\n    padding-top: 25px;\n  }\n\n  .contextual__navigation-item {\n    float: left;\n    padding-right: 40px;\n  }\n\n  .contextual__navigation-anchor {\n    border-bottom: 2px solid #fff;\n    color: var(--primary-color);\n    display: inline-block;\n    padding-bottom: 10px;\n  }\n\n  .contextual__navigation-anchor:hover,\n  .contextual__navigation-anchor:focus {\n    color: #4a4a4a;\n  }\n\n  .contextual__navigation-anchor--active {\n    border-color: #d9d9d9;\n    color: #4a4a4a;\n  }\n\n  .upcoming-events {\n    float: right;\n    padding-right: 35px;\n    margin-top: -80px;\n  }\n\n    .upcoming-events__navigation {\n      height: 30px;\n    }\n\n      .upcoming-events__up-next {\n        color: var(--primary-color-lighter);\n        display: inline-block;\n        font-size: 11px;\n        font-size: 1.1rem;\n        float: left;\n        letter-spacing: 0.2em;\n        padding-left: 2px;\n        padding-right: 30px;\n        text-transform: uppercase;\n      }\n\n      .upcoming-events__view-all {\n        color: var(--primary-color-lighter);\n        float: right;\n        font-size: 16px;\n        font-size: 1.6rem;\n      }\n\n      .upcoming-events__view-all:hover,\n      .upcoming-events__view-all:focus {\n        color: #fff;\n      }\n\n      .upcoming-events__view-all i {\n        font-size: 8px;\n        padding-left: 12px;\n        vertical-align: middle;\n      }\n\n    .upcoming-events__item {\n      border-left: 2px solid #66dacc;\n      float: left;\n      width: 187px;\n    }\n\n    .upcoming-events__item-anchor {\n      display: block;\n    }\n\n    .upcoming-events__item-anchor--hover .upcoming-events__item-image,\n    .upcoming-events__item-anchor:focus .upcoming-events__item-image {\n      transform: translateY(-50px);\n      transition-property: transform;\n      transition-duration: 100ms;\n    }\n\n    .upcoming-events__item-anchor--hover .upcoming-events__item-text-container,\n    .upcoming-events__item-anchor:focus .upcoming-events__item-text-container {\n      background-color: #f1fffd;\n    }\n\n      .upcoming-events__item-image-container {\n        display: block;\n        height: 50px;\n        width: 185px;\n        position: relative;\n      }\n\n        .upcoming-events__item-image {\n          display: block;\n        }\n\n      .upcoming-events__item-text-container {\n        background: #fff;\n        display: block;\n        padding: 15px 10px 6px;\n        position: relative;\n        z-index: 2;\n      }\n\n        .upcoming-events__item-date {\n          color: #6e6e6e;\n          font-size: 13px;\n          font-size: 1.3rem;\n          padding-bottom: 5px;\n        }\n\n        .upcoming-events__item-title {\n          color: #4a4a4a;\n          font-size: 16px;\n          font-size: 1.6rem;\n          overflow: hidden;\n          text-overflow: ellipsis;\n          white-space: nowrap;\n          width: 100%;\n        }\n",
    "deps": {}
  },
