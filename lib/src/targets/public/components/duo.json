    "src": "/* Variables */\n\n:root {\n  --column-wide: 1200px;\n  --column-mid: 940px;\n  --column-small: 860px;\n  --column-narrow: 600px;\n  --font-sans: \"Colfax\", Helvetica, sans-serif;\n  --font-serif: \"FreightTextPro\", \"Hoefler Text\", Palatino, serif;\n  --primary-color: #009e8c; /* 0, 158, 140 */\n  --primary-color-lighter: #ccefeb;\n  --primary-color-darker: color(var(--primary-color) shade(15%));\n  --text-color: #222;\n  --max-width: 100%;\n}\n\n/* Imports */\n\n@import \"./utilities/reset\";\n@import \"./utilities/accessibility\";\n@import \"./utilities/clearfix\";\n@import \"./fonts/colfax\";\n@import \"./fonts/freight-text-pro\";\n@import \"./fonts/font-awesome\";\n\n/* Base */\n\n* {\n  box-sizing: border-box;\n}\n\nhtml {\n  font-size: 62.5%;\n}\n\nbody {\n  background-color: #fff;\n  color: var(--text-color);\n  font-family: var(--font-serif);\n  font-size: 17px;\n  font-size: 1.7rem;\n  line-height: 1.3;\n  font-smoothing: antialiased;\n  -webkit-font-smoothing: antialiased;\n}\n\n@import \"./copy\";\n\n/* Basic layout */\n\n.column-wrapper {\n  padding-left: 35px;\n  padding-right: 35px;\n}\n\n.column-wide,\n.column-mid,\n.column-small,\n.column-narrow {\n  margin-left: auto;\n  margin-right: auto;\n}\n.column-wide {\n  max-width: var(--column-wide);\n}\n.column-mid {\n  max-width: var(--column-mid);\n}\n.column-small {\n  min-width: var(--column-small);\n}\n.column-narrow {\n  max-width: var(--column-narrow);\n}\n\n.block-link {\n  color: var(--primary-color);\n  display: block;\n  font-family: var(--font-sans);\n  font-weight: bold;\n  text-align: center;\n  padding-top: 35px;\n  padding-bottom: 35px;\n  margin-bottom: 35px;\n}\n  .block-link:hover {\n    color: #222;\n    text-decoration: underline;\n  }\n\n.section--normal {\n  margin-left: auto;\n  margin-right: auto;\n}\n\n.intro {\n  text-align: center;\n}\n  .intro--small {\n    font-size: 17px;\n    font-size: 1.7rem;\n  }\n\n  .section-header {\n    text-align: center;\n    padding-top: 15px;\n    padding-bottom: 35px;\n  }\n\n  .section-header h2 {\n    color: #777;\n    font-family: var(--font-sans);\n    -webkit-font-smoothing: subpixel-antialiased;\n    font-smoothing: subpixel-antialiased;\n    font-size: 14px;\n    font-size: 1.4rem;\n    letter-spacing: 0.15em;\n    text-transform: uppercase;\n    padding-bottom: 12px;\n  }\n\n.page__intro {\n  padding-bottom: 35px;\n}\n  .page__intro.copy {\n    font-size: 25px;\n    font-size: 2.5rem;\n  }\n\n/* Components */\n\n@import \"./masthead\";\n@import \"./masthead-contextual\";\n@import \"./footer\";\n@import \"./pagination\";\n@import \"./figure\";\n@import \"./sub-page-nav\";\n@import \"./video-player\";\n@import \"./subscribe\";\n@import \"./topics-section\";\n@import \"./blog-post\";\n@import \"./topic\";\n",
