-left: auto;\n  margin-right: auto;\n  height: 75px;\n  width: 75px;\n}\n  .author-image img {\n    display: block;\n    width: 100%;\n  }\n  .author-image__inner {\n    border-radius: 50%;\n    height: 100%;\n    overflow: hidden;\n    width: 100%;\n  }\n\n.blog-index__guest-authors {\n  background-color: #dbfffb;\n  clear: both;\n  overflow: hidden;\n  padding-top: 35px;\n}\n.blog-index__guest-authors-inner {\n  margin-left: -20px; /* Offset the internal padding for each author */\n  margin-right: -20px;\n}\n\n.guest-author {\n  padding-bottom: 45px;\n}\n  .guest-author .blog-post-list:before,\n  .guest-author .blog-post-list__anchor {\n    border-color: color(#dbfffb shade(5%));;\n  }\n  .breakpoint-widescreen-wide .guest-author,\n  .breakpoint-widescreen .guest-author {\n    float: left;\n    width: 25%;\n    padding-left: 20px;\n    padding-right: 20px;\n  }\n\n.guest-author__title {\n  font-size: 18px;\n  font-size: 1.8rem;\n  font-family: var(--font-sans);\n  font-weight: bold;\n  text-align: center;\n  padding-top: 15px;\n  padding-bottom: 15px;\n}\n\n.guest-author__intro {\n  font-size: 16px;\n  font-size: 1.6rem;\n  text-align: center;\n}\n\n\n/* Blog post list */\n\n.blog-post-list {\n  position: relative;\n}\n  .blog-post-list:before {\n    border-top: 1px solid #dfdfdf;\n    content: \"\";\n    display: block;\n    margin-left: auto;\n    margin-right: auto;\n    padding-top: 10px;\n    margin-top: 15px;\n    width: 30%;\n  }\n\n  .blog-post-list__anchor {\n    border-top: 1px solid #dfdfdf;\n    display: block;\n    padding-top: 15px;\n    padding-bottom: 15px;\n  }\n    .blog-post-list__anchor:hover .title-text {\n      color: var(--primary-color);\n      text-decoration: underline;\n    }\n    .blog-post-list li:first-child .blog-post-list__anchor {\n      border-top: none;\n    }\n\n.blog-post-list__title {\n  font-family: var(--font-sans);\n  font-size: 15px;\n  font-size: 1.5rem;\n  font-weight: bold;\n}\n\n.blog-snap__group {\n  border-bottom: 1px solid #dfdfdf;\n  margin-bottom: 35px;\n}\n  .blog-snap__group + .block-link {\n    margin-top: -36px;\n  }\n\n.blog-snap-halves,\n.blog-snap-quarters {\n  float: left;\n}\n\n.blog-snap-halves--1,\n.blog-snap-quarters--2 {\n  width: 50%;\n}\n\n\n.blog-snap--half,\n.blog-snap--quarter {\n  margin-right: 35px;\n  float: left;\n}\n\n.blog-snap-halves--2 .blog-snap,\n.blog-snap-quarters--2 .blog-snap {\n  width: 50%;\n  width: calc(50% - 35px);\n}\n\n.blog-snap-quarters--3 .blog-snap {\n  width: 33%;\n  width: calc(33% - 35px);\n}\n\n.blog-snap__group--4 .blog-snap {\n  width: 25%;\n}\n.blog-snap-quarters--4 .blog-snap {\n  width: 25%;\n  width: calc(25% - 35px);\n}\n\n\n\n/* */\n\n.gradient-top-down {\n  background: -webkit-gradient(linear, 0% 50%, 100% 50%, color-stop(0%, #fff), color-stop(20%, rgba(250, 250, 250, 0))), -webkit-gradient(linear, 100% 50%, 0% 50%, color-stop(0%, #fff), color-stop(20%, rgba(250, 250, 250, 0))), -webkit-gradient(linear, 50% 100%, 50% 0%, color-stop(0%, transparent), color-stop(100%, rgba(0, 0, 0, 0.05)));\n  background: -webkit-linear-gradient(left, #fff 0%, rgba(250, 250, 250, 0) 20%), -webkit-linear-gradient(right, #fff 0%, rgba(250, 250, 250, 0) 20%), -webkit-linear-gradient(bottom, transparent, rgba(0, 0, 0, 0.05));\n  background: -moz-linear-gradient(left, #fff 0%, rgba(250, 250, 250, 0) 20%), -moz-linear-gradient(right, #fff 0%, rgba(250, 250, 250, 0) 20%), -moz-linear-gradient(bottom, transparent, rgba(0, 0, 0, 0.05));\n  background: -o-linear-gradient(left, #fff 0%, rgba(250, 250, 250, 0) 20%), -o-linear-gradient(right, #fff 0%, rgba(250, 250, 250, 0) 20%), -o-linear-gradient(bottom, transparent, rgba(0, 0, 0, 0.05));\n  background: linear-gradient(left, #fff 0%, rgba(250, 250, 250, 0) 20%), linear-gradient(right, #fff 0%, rgba(250, 250, 250, 0) 20%), linear-gradient(bottom, transparent, rgba(0, 0, 0, 0.05));\n  padding-top: 35px;\n}\n\n.blog-index__topic {\n  padding-top: 35px;\n  padding-bottom: 35px;\n}\n\n.blog-snap--block {