    "src": ".blog-post {\n  padding-top: 70px;\n  padding-bottom: 50px;\n}\n\n.blog-post__header {\n  font-family: var(--font-sans);\n  text-align: center;\n  margin-bottom: 50px;\n}\n\n  .blog-post__byline {\n    color: #888;\n    font-size: 15px;\n    font-size: 1.5rem;\n    margin-bottom: 15px;\n  }\n    .blog-post__byline a {\n      text-decoration: underline;\n      color: #666;\n    }\n      .blog-post__byline a:hover,\n      .blog-post__byline a:focus {\n        color: #222;\n      }\n\n    .blog-post__byline-separator {\n      padding-left: 12px;\n      padding-right: 12px;\n    }\n\n  .blog-post__title {\n    color: var(--primary-color);\n    font-size: 48px;\n    font-size: 4.8rem;\n    font-weight: bold;\n  }\n\n.blog-post__intro {\n  padding-top: 50px;\n  padding-bottom: 15px;\n}\n  .blog-post__intro:after {\n    content: \"\";\n    border-bottom: 1px solid #dfdfdf;\n    display: block;\n    margin-left: auto;\n    margin-right: auto;\n    margin-top: 40px;\n    margin-bottom: 25px;\n    width: 30%;\n  }\n\n  .blog-post__body .section--full {\n    padding-bottom: 30px;\n    padding-top: 30px;\n    margin-left: -35px;\n    margin-right: -35px;\n  }\n\n  .blog-post__intro .section--normal,\n  .blog-post__body .section--normal {\n    max-width: var(--column-mid);\n    position: relative;\n  }\n  .breakpoint-widescreen-wide .blog-post__body .section--normal > *:not(div),\n  .breakpoint-widescreen .blog-post__body .section--normal > *:not(div),\n  .breakpoint-tablet-wide .blog-post__body .section--normal > *:not(div) {\n    /* Shift _most_ of the content over */\n    margin-right: 25%;\n    margin-right: calc(25% + 3rem);\n  }\n  /* Hide the intro line when there's a full-width section as the first thing in the body */\n  .blog-post__intro + .blog-post__body .section--full:first-child .figure:first-child {\n    margin-top: -71px;\n  }\n\n\n.blog-post__meta {\n  padding-top: 30px;\n}\n  .blog-post__meta-inner {\n    color: #888;\n    font-family: var(--font-sans);\n    font-size: 17px;\n    font-size: 1.7rem;\n  }\n  .blog-post__meta-inner:before {\n    border-bottom: 1px solid #dfdfdf;\n    content: \"\";\n    display: block;\n    margin-bottom: 35px;\n    width: 15%;\n  }\n\n.blog-post .figure__display--default {\n  padding-bottom: 30px;\n  padding-top: 20px;\n}\n  .breakpoint-widescreen-wide .blog-post .figure__display--default {\n    margin-left: -25%;\n    margin-right: -25%;\n  }\n\n.blog-post .figure__display--left-aligned {\n  float: left;\n  margin-right: 35px;\n  padding-bottom: 35px;\n  padding-top: 4px;\n  width: 35%;\n}\n  .breakpoint-widescreen-wide .blog-post .figure__display--left-aligned {\n    margin-left: -25%;\n    width: 50%;\n  }\n\n.blog-post .figure__display--right-aligned {\n  float: right;\n  width: 50%;\n  margin-left: 35px;\n  padding-bottom: 35px;\n  padding-top: 4px;\n}\n  .breakpoint-widescreen-wide .blog-post .figure__display--right-aligned {\n    margin-right: -25%;\n  }\n\n/* Tags */\n.blog-post__tags {\n  color: #888;\n  font-family: var(--font-sans);\n  padding-top: 50px;\n  position: relative;\n}\n  .blog-post__tag-header {\n    font-size: 12px;\n    font-size: 1.2rem;\n    letter-spacing: 0.15em;\n    padding-right: 20px;\n    padding-top: 2px;\n    margin-left: -81px;\n    position: absolute;\n    text-transform: uppercase;\n  }\n  .blog-post__tags-list {\n    display: block;\n    font-size: 15px;\n    font-size: 1.5rem;\n  }\n  .blog-post__tags-list li {\n    display: inline;\n  }\n  .blog-post__tags-list li:after {\n    content: \", \";\n    margin-left: -0.2em;\n  }\n    .blog-post__tags-list li:last-child:after {\n      content: \"\";\n    }\n  .blog-post__tags-list a {\n    color: #222;\n    text-decoration: underline;\n  }\n    .blog-post__tags-list a:hover {\n      color: var(--primary-color);\n    }\n\n\n\n/*Index*/\n\n.blog-index {\n  padding-top: 60px;\n  padding-bottom: 100px;\n}\n\n/* Latest */\n.blog-index__latest {}\n\n  .blog-index__latest-header {\n    color: #777;\n    font-family: var(--font-sans);\n    -webkit-font-smoothing: subpixel-antialiased;\n    font-smoothing: subpixel-antialiased;\n    font-size: 14px;\n    font-size: 1.4rem;\n    letter-spacing: 0.15em;\n    text-transform: uppercase;\n    text-align: center;\n    margin-bottom: 35px;\n  }\n\n\n.blog-snap {\n  display: block;\n  padding-bottom: 15px;\n}\n\n  .blog-snap__date {\n    color: #888;\n    display: block;\n    font-family: var(--font-sans);\n    font-size: 1.4rem;\n    font-size: 14px;\n    padding-bottom: 8px;\n  }\n\n  .blog-snap__title {\n    font-family: var(--font-sans);\n    font-size: 23px;\n    font-size: 2.3rem;\n    font-weight: bold;\n    padding-top: 6px;\n    margin-bottom: 14px;\n    line-height: 1.2;\n  }\n    .blog-snap--quarter .blog-snap__title {\n      font-size: 19px;\n      font-size: 1.9rem;\n    }\n    .blog-snap--block .blog-snap__title {\n      font-size: 16px;\n      font-size: 1.6rem;\n      line-height: 1.4;\n    }\n\n    .blog-snap__title .title-tags {\n      font-size: 17px;\n      font-size: 1.7rem;\n    }\n    .title-tags {\n      color: #888;\n      font-family: var(--font-serif);\n      font-weight: normal;\n    }\n\n    .blog-snap:hover .title-text {\n      color: var(--primary-color);\n      text-decoration: underline;\n    }\n\n  .blog-snap__authors {\n    display: block;\n    font-family: var(--font-sans);\n    font-size: 1.4rem;\n    font-size: 14px;\n    padding-bottom: 12px;\n    margin-top: -10px;\n  }\n\n  .blog-snap__summary {\n    font-size: 19px;\n    font-size: 1.9rem;\n  }\n    .blog-snap--block .blog-snap__summary,\n    .blog-snap--quarter .blog-snap__summary {\n      font-size: 16px;\n      font-size: 1.6rem;\n    }\n\n  .blog-snap__hero {\n    padding-bottom: 4px;\n    position: relative;\n  }\n  .blog-snap__hero-type {\n    background-color: #fff;\n    font-family: var(--font-sans);\n    font-size: 13px;\n    position: absolute;\n    top: 0;\n    right: 0;\n    padding: 7px 8px 8px;\n    /* Needs to keep